{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "LogsTableName": "Logs",
    "MetricsTableName": "Metrics",
    "TracesTableName": "Traces",
    "ApplicationsTableName": "Applications"
  },
  "Collector": {
    "BatchSize": 100,
    "FlushIntervalMs": 5000,
    "MaxQueueSize": 10000
  },
  "ApiKeys": {
    "Enabled": false,
    "Keys": []
  }
}
